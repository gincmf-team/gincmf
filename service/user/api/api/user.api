syntax = "v1"

type (
    UserReq {
        Id string `path:"id"`
    }
)

@server(
    group: user
    prefix: /api/v1/admin/user
)

service user {
    @server(
        handler: Index
    )
    get / returns (Response)

    @server(
        handler: Get
    )
    get /:id (UserReq) returns (Response)
}
